 @ Carry Out Eval Analysis at each date @
 eval_mat = miss(zeros(dnobs,1),0);
 t=1; do while t <= ir_last-n_eig;
 	tmp = yres[t:t+n_eig-1,.];
 	tmp = packr(tmp');
 	if rows(tmp) .> 100;
   cov = tmp'tmp;
   eval = eigh(cov);
   eval = sortc(eval,1);
   eval_mat[t]=eval[n_eig]/sumc(eval);
  endif;
 t=t+1; endo;
 eval_mat = packr(eval_mat);
 
