  ii = (calvec .> c1-.0001);
  tmp = selif(yres,ii);
  tmp = tmp[1:n_eig,.];
  tmp = packr(tmp');
  cov = tmp'tmp;
  eval = eigh(cov);
  eval = sortc(eval,1);
  tmp = eval[n_eig]/sumc(eval);
  c1;;tmp;; meanc(eval_mat .>= tmp);
