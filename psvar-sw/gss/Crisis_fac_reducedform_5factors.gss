 output file = ^str reset; output on; 
 "";"";
 "Andrews EOS Test -- Lamda";
 "";
 "Num, Series, 1959-2007Q3, 1984Q1-2007Q3";

 alambda_tst = miss(zeros(nx,2),1);  @ Full Sample and Post 1983 @
 i=1; do while i <= nx;
 	y=ydata[.,i];
 	x=fac_rf;
 	c1 = 2007 + (4-1)/4;
 	ii = calvec .> (c1-.0001);
  tmp = selif(y~x,ii);
  tmp=packr(tmp);
  if ismiss(tmp) .== 0;
   m = rows(tmp);
   tmp = packr(y~x);
   yp=tmp[.,1];
   xp=tmp[.,2:cols(tmp)];
   alambda_tst[i,1]=andrews_eos(yp,xp,m);
   c1 = 1984 + (1-1)/4;
   ii = (calvec .> c1-.0001); 
   y = selif(y,ii);
   x = selif(x,ii);
   tmp = packr(y~x);
